{
    "name": "akshitarora/dblog",
    "description": "Tool to log heavy database queries in the Laravel application",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Akshit Arora",
            "email": "akshit.arora0907@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "AkshitArora\\DbLog\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AkshitArora\\DbLog\\Tests\\" : "tests"
        }
    },
    "require": {},
    "require-dev": {
        "composer/composer": "^2.1",
        "phpunit/phpunit": "^9.5",
        "illuminate/support": "^8.18|^9.0",
        "orchestra/testbench": "^6.17 | ^7.0",
        "pestphp/pest": "^1.21",
        "laradumps/laradumps": "^1.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "AkshitArora\\DbLog\\DbLogServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
